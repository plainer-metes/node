# Copy this file to .env before running locally
# cp .env.example .env

# Local or remote RPC endpoint
RPC_URL=http://localhost:8545

# Network (Ethereum mainnet or any test network like Rinkeby, Sepolia, etc.)
NETWORK=development

# Optional API key (if your environment requires one)
API_KEY=

# Logging level: info, debug, or warn
LOG_LEVEL=info
